<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="com.cardDetailsApplication.Model.CardDetails" table="CardDetails">
      <id name="cardId" type="integer" column="cardId">
         <generator class="increment"/>
      </id>
      <property name="cardHolderName" column="cardHolderName" type="string"/>
      <property name="cardNumber" column="cardNumber" type="string"/>
      <property name="expiryDate" column="expiryDate" type="string"/>
      <property name="userId" column="userId" type="integer"/>
<!--       <property name="salary" column="salary" type="integer"/> -->
   </class>
</hibernate-mapping>